
public class TesteReferencias {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//NOME CPF SALARIO
		Gerente g1 = new Gerente("Marcos","23214124-2", 5000.00);
		
		ControleBonificacao  controle = new ControleBonificacao();
		
		controle.registra(g1);
		
		System.out.println(controle.getSoma());
		
Gerente g2 = new Gerente("Marcos","23214124-2", 5000.00);
		
		ControleBonificacao  controle2 = new ControleBonificacao();
		
		controle.registra(g2);
		
		System.out.println(controle2.getSoma());
		
	}

}
