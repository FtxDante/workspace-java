//Gerente é um funcionario
public class EditorVideo extends Funcionario {
		
	public EditorVideo(String nome, String cpf, double salario) {
		super(nome, cpf, salario);
		// TODO Auto-generated constructor stub
	}
	
	public double getBonificacao() {
		
		double bonificacao = super.getBonificacao() + 100;
		System.out.println("Chamando o metodo de bonificacao do Editor de Video: " + bonificacao);
		return bonificacao;
	}
	
	
}
