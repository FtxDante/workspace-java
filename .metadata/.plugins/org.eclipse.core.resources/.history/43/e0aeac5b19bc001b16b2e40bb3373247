
public class TesteReferencias {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//NOME CPF SALARIO
		
		ControleBonificacao  controle = new ControleBonificacao();
		Funcionario f = new Funcionario("Adriano","45214124-2", 2000.00);
		Funcionario g1 = new Gerente("Marcos","23214124-2", 5000.00);
		Funcionario ev = new EditorVideo("Marcio", "41241241-3", 2500.00);
		Funcionario d = new Designer("Jennifer","0956341323-2",2000.00);
		
		controle.registra(g1);
		controle.registra(f);
		controle.registra(ev);
		controle.registra(d);
		
		System.out.println(controle.getSoma());
		
	}

}
